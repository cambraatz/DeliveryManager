@media only screen and (min-width:480px) {
    #MDPU_Row {
        display: flex;
        justify-content: space-between;
        padding: 0 10%;
    }

    #SPM_Row {
        display: flex;
        justify-content: space-between;
        padding: 0 12%;
    }

    #Delivery_Input_Div {
        margin: 0 15%;
    }

        #Delivery_Input_Div button {
            margin: 15px 0;
            width: 95%;
        }

    .input_form {
        width: 95%;
    }

    .Table_Separator {
        border-bottom: 1px solid #000;
        line-height: 0.1em;
        padding: 0 2%;
    }

        .Table_Separator span {
            background: #fff;
            padding: 0 10px;
        }

    .Delivery_Table {
        width: 100%;
        padding: 0 2%;
    }

    .delivered_items {
        color: rgb(170, 170, 170);
    }

    .Table_Body:hover {
        background-color: #e0eaf3;
    }

    #datetime_Div {
        display: flex;
        align-items: center;
        padding: 10px 0;
        justify-content: space-between;
    }

        #datetime_Div div {
            display: flex;
            align-items: center;
        }

        #datetime_Div input {
            width: 110px;
            height: 2em;
        }

        #datetime_Div label {
            margin-right: 10px;
        }

    #pis_Div {
        display: flex;
        align-items: center;
        padding: 10px 0;
        justify-content: space-between;
    }
}