/* Newly added CSS styling */

:root {
    --primary-color: #5A89BB;
    --action-color: #e0eaf3;
}

.App {
    color: #353535;
    background-color: white;
    text-align: center;
    font-family: 'Verdana', Verdana, Geneva, Tahoma, sans-serif;
    overflow: scroll;
}

.weak {
    font-weight: lighter;
    margin: 0;
}

