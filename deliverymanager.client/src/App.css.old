#webpage {
    margin: 60px 0;
}

h2 {
    margin: 1em 0;
}

h3 {
    margin: 0.75em 0;
}

h4 {
    margin: 0.75em 0;
}

header h1, header h2, header h3, header h4, header h5 {
    margin: 16px 15px 0 15px;
    display: flex;
    align-items: start;
}

.SPM_Col h4, .SPM_Col h5 {
    margin: 0 20px;
    display: flex;
    align-items: start;
}

#AccountTab {
    display: flex;
    padding: 0 20px 8px 20px;
    justify-content: space-between;
    z-index: 100;
}

#popupLogoutInner {
    display: flex;
    flex-direction: column;
}

.overlay {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.7);
    transition: opacity 500ms;
    visibility: hidden;
    opacity: 0;
}

    .overlay:target {
        visibility: visible;
        opacity: 1;
    }

#popupLogoutWindow {
    visibility: hidden;
    opacity: 0;
    pointer-events: none;
}

.popupLogout {
    margin: 70px auto;
    padding: 5px 20px;
    background: #fff;
    border-radius: 5px;
    width: 50%;
    position: relative;
    transition: all 5s ease-in-out;
}

    .popupLogout .close {
        position: absolute;
        top: 20px;
        right: 30px;
        transition: all 200ms;
        font-size: 30px;
        font-weight: bold;
        text-decoration: none;
        color: #333;
    }

        .popupLogout .close:hover {
            color: #06D85F;
        }

    .popupLogout .content {
        max-height: 30%;
        overflow: auto;
    }

    .popupLogout h1 {
        display: flex;
        flex-direction: row-reverse;
        margin: 0;
        margin-block-start: 0;
        margin-block-end: 0;
        margin-inline-start: 0;
        margin-inline-end: 0;
    }

    .popupLogout p {
        margin: 0;
        margin-block-start: 0;
        margin-block-end: 0.75em;
        margin-inline-start: 0;
        margin-inline-end: 0;
    }

#popupLogoutPrompt {
    display: flex;
    justify-content: center;
}

#popupLoginWindow {
    visibility: hidden;
    opacity: 0;
    pointer-events: none;
}

.popupLogin {
    margin: 70px auto;
    padding: 5px 20px;
    background: #fff;
    border-radius: 5px;
    width: 50%;
    position: relative;
    transition: all 5s ease-in-out;
}

    .popupLogin .close {
        position: absolute;
        top: 20px;
        right: 30px;
        transition: all 200ms;
        font-size: 30px;
        font-weight: bold;
        text-decoration: none;
        color: #333;
    }

        .popupLogin .close:hover {
            color: #06D85F;
        }

    .popupLogin .content {
        max-height: 30%;
        overflow: auto;
    }

    .popupLogin h1 {
        display: flex;
        flex-direction: row-reverse;
        margin: 0;
        margin-block-start: 0;
        margin-block-end: 0.25em;
        margin-inline-start: 0;
        margin-inline-end: 15px;
    }

    .popupLogin p {
        margin: 0;
        margin-block-start: 0;
        margin-block-end: 0.75em;
        margin-inline-start: 0;
        margin-inline-end: 0;
    }

#popupLoginPrompt {
    display: flex;
    justify-content: center;
}

.popupLoginContent {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.popupLoginContent div {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 0.75em;
}

.popupLoginContent input, .popupLoginContent label {
    width: 80%;
    text-align: start;
}

#popupLoginInner {
    text-align: center;
}

#widgetHeader {
    display: block;
    text-align: center;
    margin: 16px 1em 0 1em;
    border-top: solid 1px black;
    border-left: solid 1px black;
    border-right: solid 1px black;
    border-radius: 5px 5px 0 0;
    background-color: #5A89BB;
    color: white;
}

.prompt {
    display: block;
    padding: 8px 0;
}

#widgetHeader h4 {
    margin: 0;
}



